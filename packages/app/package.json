{
  "name": "app",
  "version": "0.0.0",
  "private": true,
  "bundled": true,
  "backstage": {
    "role": "frontend"
  },
  "scripts": {
    "start": "set PORT=3200 && backstage-cli package start",
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "test": "backstage-cli package test",
    "lint": "backstage-cli package lint",
    "test:e2e": "cross-env PORT=3001 start-server-and-test start http://localhost:3001 cy:dev",
    "test:e2e:ci": "cross-env PORT=3001 start-server-and-test start http://localhost:3001 cy:run",
    "cy:dev": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "build-image": "docker build ../.. -f Dockerfile --tag backstage"
  },
  "dependencies": {
    "@backstage/app-defaults": "^1.2.1",
    "@backstage/backend-common": "0.15.0",
    "@backstage/backend-defaults": "0.2.18",
    "@backstage/catalog-model": "^1.5.0",
    "@backstage/cli": "^0.33.1",
    "@backstage/core-app-api": "^1.12.0",
    "@backstage/core-components": "^0.17.4",
    "@backstage/core-plugin-api": "^1.9.0",
    "@backstage/integration-react": "^1.1.23",
    "@backstage/plugin-api-docs": "^0.9.1",
    "@backstage/plugin-auth-backend": "0.25.2",
    "@backstage/plugin-catalog": "^1.9.0",
    "@backstage/plugin-catalog-common": "^1.0.22",
    "@backstage/plugin-catalog-graph": "^0.2.28",
    "@backstage/plugin-catalog-import": "^0.10.7",
    "@backstage/plugin-catalog-react": "^1.11.0",
    "@backstage/plugin-github-actions": "^0.6.13",
    "@backstage/plugin-org": "^0.6.6",
    "@backstage/plugin-permission-backend": "0.7.2",
    "@backstage/plugin-permission-react": "^0.4.21",
    "@backstage/plugin-scaffolder": "^1.12.0",
    "@backstage/plugin-search": "^1.1.1",
    "@backstage/plugin-search-react": "^1.5.1",
    "@backstage/plugin-techdocs": "^1.6.0",
    "@backstage/plugin-techdocs-module-addons-contrib": "^1.0.11",
    "@backstage/plugin-techdocs-react": "^1.1.4",
    "@backstage/plugin-user-settings": "^0.7.1",
    "@backstage/theme": "^0.6.7",
    "@mui/icons-material": "^5.12.2",
    "@mui/material": "^5.12.2",
    "@mui/styles": "^5.12.2",
    "history": "^5.0.0",
    "lerna": "8.2.3",
    "react": "^18.0.2",
    "react-dom": "^18.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.2.4"
  },
  "devDependencies": {
    "@backstage/test-utils": "^1.5.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react-dom": "*",
    "cross-env": "^7.0.0",
    "cypress": "^13.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "start-server-and-test": "^2.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "files": [
    "dist"
  ]
}
